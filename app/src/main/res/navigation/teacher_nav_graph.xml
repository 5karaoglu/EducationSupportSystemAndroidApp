<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/teacher_nav_graph">

    <fragment
        android:id="@+id/teacherAddFragment"
        android:name="com.example.ess.ui.teacher.add.TeacherAddFragment"
        android:label="fragment_teacher_add"
        tools:layout="@layout/fragment_teacher_add" >
        <action
            android:id="@+id/action_teacherAddFragment_to_teacherEditTitleFragment"
            app:destination="@id/teacherEditTitleFragment" />
        <action
            android:id="@+id/action_teacherAddFragment_to_teacherSubmitsFragment"
            app:destination="@id/teacherSubmitsFragment" />
        <action
            android:id="@+id/action_teacherAddFragment_to_teacherCreateIssueFragment"
            app:destination="@id/teacherCreateIssueFragment" />
    </fragment>
    <fragment
        android:id="@+id/teacherNotificationFragment"
        android:name="com.example.ess.ui.teacher.notification.TeacherNotificationFragment"
        android:label="fragment_teacher_notification"
        tools:layout="@layout/fragment_teacher_notification" >
        <action
            android:id="@+id/action_teacherNotificationFragment_to_teacherPushNotification"
            app:destination="@id/teacherPushNotification" />
    </fragment>
    <fragment
        android:id="@+id/teacherProfileFragment"
        android:name="com.example.ess.ui.teacher.profile.TeacherProfileFragment"
        android:label="fragment_teacher_profile"
        tools:layout="@layout/fragment_teacher_profile" >
        <action
            android:id="@+id/action_teacherProfileFragment_to_teacherEditProfileFragment"
            app:destination="@id/teacherEditProfileFragment" />
        <argument
            android:name="userShort"
            app:argType="com.example.ess.model.UserShort" />
    </fragment>
    <fragment
        android:id="@+id/teacherPushNotification"
        android:name="com.example.ess.ui.teacher.notification.TeacherPushNotification"
        android:label="fragment_teacher_push_notification"
        tools:layout="@layout/fragment_teacher_push_notification" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.ess.ui.common.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_showProfileFragment"
            app:destination="@id/showProfileFragment" />
        <argument
            android:name="userShort"
            app:argType="com.example.ess.model.UserShort" />
    </fragment>
    <fragment
        android:id="@+id/teacherEditProfileFragment"
        android:name="com.example.ess.ui.teacher.profile.TeacherEditProfileFragment"
        android:label="fragment_teacher_edit_profile"
        tools:layout="@layout/fragment_teacher_edit_profile" >
        <action
            android:id="@+id/action_teacherEditProfileFragment_to_teacherProfileFragment"
            app:destination="@id/teacherProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/teacherEditTitleFragment"
        android:name="com.example.ess.ui.teacher.add.TeacherEditTitleFragment"
        android:label="fragment_teacher_edit_title"
        tools:layout="@layout/fragment_teacher_edit_title" >
        <action
            android:id="@+id/action_teacherEditTitleFragment_to_teacherAddFragment"
            app:destination="@id/teacherAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/teacherSubmitsFragment"
        android:name="com.example.ess.ui.teacher.add.TeacherSubmitsFragment"
        android:label="fragment_teacher_submits"
        tools:layout="@layout/fragment_teacher_submits" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.ess.ui.common.messages.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" >
        <argument
            app:argType="com.example.ess.model.Contact"
            android:name="contact" />
    </fragment>
    <fragment
        android:id="@+id/messagesFragment"
        android:name="com.example.ess.ui.common.messages.MessagesFragment"
        android:label="fragment_messages"
        tools:layout="@layout/fragment_messages" >
        <action
            android:id="@+id/action_messagesFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <argument
            app:argType="com.example.ess.model.Contact"
            android:name="contact"/>
    </fragment>
    <fragment
        android:id="@+id/showProfileFragment"
        android:name="com.example.ess.ui.common.showprofile.ShowProfileFragment"
        android:label="fragment_show_profile"
        tools:layout="@layout/fragment_show_profile" >
        <argument
            android:name="userShort"
            app:argType="com.example.ess.model.UserShort"
            app:nullable="true"/>
        <argument
            android:name="feedItem"
            app:argType="com.example.ess.model.FeedItem"
            app:nullable="true" />
        <action
            android:id="@+id/action_showProfileFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/commentsFragment"
        android:name="com.example.ess.ui.teacher.home.CommentsFragment"
        android:label="fragment_comments"
        tools:layout="@layout/fragment_comments" >
        <argument
            android:name="feedItem"
            app:argType="com.example.ess.model.FeedItem"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/submitsFragment"
        android:name="com.example.ess.ui.teacher.home.SubmitsFragment"
        android:label="fragment_submits"
        tools:layout="@layout/fragment_submits" >
        <argument
            android:name="feedItem"
            app:argType="com.example.ess.model.FeedItem"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/teacherCreateIssueFragment"
        android:name="com.example.ess.ui.teacher.add.TeacherCreateIssueFragment"
        android:label="fragment_teacher_create_issue"
        tools:layout="@layout/fragment_teacher_create_issue" >
        <action
            android:id="@+id/action_teacherCreateIssueFragment_to_teacherAddFragment"
            app:destination="@id/teacherAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.ess.ui.common.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_teacherHomeFragment_to_messagesFragment"
            app:destination="@id/messagesFragment" />
        <action
            android:id="@+id/action_teacherHomeFragment_to_commentsFragment"
            app:destination="@id/commentsFragment" />
        <action
            android:id="@+id/action_teacherHomeFragment_to_submitsFragment"
            app:destination="@id/submitsFragment" />
        <action
            android:id="@+id/action_teacherHomeFragment_to_showProfileFragment"
            app:destination="@id/showProfileFragment" />
        <argument
            android:name="feedItem"
            app:argType="com.example.ess.model.FeedItem"
            app:nullable="true" />
    </fragment>
</navigation>